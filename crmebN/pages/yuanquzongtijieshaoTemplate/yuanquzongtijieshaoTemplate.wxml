<template name="content">
<view id="wrap">
<!--园区总体介绍标题加图标返回-->
<view class="wrap_title bg-white">
  <view class="cu-custom" style="height:{{CustomBar}}px;">
    <view class="cu-bar fixed bg-white" style="height:{{CustomBar}}px;padding-top:{{StatusBar}}px;">
      <navigator class='action' open-type="navigateBack" delta="1" hover-class="none">
        <text class='icon-iconfontback' style="font-size:20px"></text>  <text class='text-xxl text-bold'>{{backNavTitle}}</text>
      </navigator>
    </view>
  </view>
</view>
<!--金龟岭各园区介绍-->
<view class="padding solid-top text-center text-xxl text-bold bg-white" style="margin-top:{{StatusBar}}px;">{{Title}}</view>
<!--背景图片-->
<view class="" style="background-image:url({{bgImagesUrl}});width:100%;height:500rpx;background-size:100% 100%"></view>
<!--定位图片-->
<view style="position:absolute;top:600rpx;left:0;width:100%">
    <view class="bg-white padding" style="width:90%;margin: 0 auto;border-radius:15rpx;box-shadow:0 0 15rpx #ccc">
        <view style="width:100%;">
           <block wx:for="{{oneImagesList}}" wx:if="{{ index < oneImagesList.length-1 }}" wx:key>
              <view class="" style="background-image:url({{item}});width:100%;height:285rpx;margin-bottom:20rpx;border-radius:15rpx;background-size:100% 100%"></view>
           </block>
           <view>{{desc}}</view>
        </view>

         <block wx:for="{{positionImgJson}}" wx:key>
            <view class="flex justify-between" style="width:100%;margin-top:20rpx">
                <block wx:for="{{positionImgJson[index]['img']}}" wx:key>
                    <view class="" style="background-image:url({{item}});width:49%;height:285rpx;margin-bottom:20rpx;border-radius:15rpx;background-size:100% 100%"></view>
                </block>
            </view>
        
            <view style="width:100%;margin-bottom:20rpx;">
              <block wx:for="{{positionImgJson[index]['title']}}" wx:key>
                <view wx:if="{{item}}">{{item}}</view>
              </block>
            </view>
        </block>
      
    </view>
</view>

</view>
</template>